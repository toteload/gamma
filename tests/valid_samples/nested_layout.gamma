layout Monster
  head: 0 Head
  limbs: 8 u32
end

layout Head
  eyes: 0 u32
  teeth: 4 u32
end

fn main(): i32
  let beholder: Monster

  set beholder.head.eyes = (cast u32 1)
  set beholder.head.teeth = (cast u32 123)
  set beholder.limbs = (cast u32 8)

  return (cast i32 beholder.head.teeth)
end
