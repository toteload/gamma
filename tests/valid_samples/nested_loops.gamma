fn main(): i32
    let i: i32 = (cast i32 0)

    loop
        if (eq i (cast i32 10))
            break
        end

        let j: i32 = (cast i32 0)

        loop
            if (eq j (cast i32 10))
                break
            end

            set j = (+ j (cast i32 1))
        end

        set i = (+ i (cast i32 1))
    end

    return i
end
